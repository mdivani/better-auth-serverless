{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "Auth service - Lambda Wrapper for BetterAuth",
  "author": "Giorgi Mdivani",
  "main": "serverless.ts",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "6.12.0",
    "aws-lambda": "1.0.7",
    "aws-serverless-express": "3.4.0",
    "better-auth": "1.3.2",
    "cors": "2.8.5",
    "express": "5.1.0"
  },
  "devDependencies": {
    "@serverless/typescript": "4.17.1",
    "@types/aws-lambda": "8.10.109",
    "@types/aws-serverless-express": "3",
    "@types/cors": "2",
    "@types/express": "5",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.2.6",
    "@types/node": "22.10.0",
    "@types/serverless-jest-plugin": "0.3.3",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "esbuild": "0.17.4",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "jest": "29.3.1",
    "jest-junit": "15.0.0",
    "prettier": "3.4.2",
    "prisma": "6.12.0",
    "serverless": "4.17.1",
    "serverless-domain-manager": "8.0.0",
    "serverless-esbuild": "1.55.1",
    "serverless-export-env": "2.2.0",
    "serverless-offline": "14.4.0",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.3"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "deploy": "serverless deploy",
    "start": "npx serverless export-env && npx serverless offline",
    "export-env": "npx serverless export-env",
    "format": "prettier . --write",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "generate:better-auth": "npx @better-auth/cli generate",
    "generate:manual": "npx prisma migrate dev --create-only"
  },
  "engines": {
    "node": ">=22.10.0"
  },
  "packageManager": "yarn@4.3.1"
}
